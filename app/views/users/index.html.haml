%h1 Users
%table.last
  %thead
    %tr
      %th Email
      %th Admin
      %th Identities
  %tbody
    - @users.each do |user|
      %tr{:class => cycle("odd", "even")}
        %td= link_to user.email, user_url(user)
        %td= user.admin
        %td= user.identities.size

= will_paginate @users